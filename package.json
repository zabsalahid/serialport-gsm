	"author": "Zabdiel G. Salahid <zabsalahid@gmail.com> (https://github.com/zabsalahid)",
	"repository": {
		"type": "git",
		"url": "https://github.com/zabsalahid/serialport-gsm.git"
	},
	"bugs": {
		"url": "https://github.com/zabsalahid/serialport-gsm/issues"
	},
	"keywords": [
		"gsm",
		"at",
		"commands",
		"modem",
		"sms",
		"iot",
		"serial port",
		"serialport",
		"serial",
		"com",
		"port",
		"flash message",
		"class 0"
	]
